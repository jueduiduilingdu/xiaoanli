<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="RoundProgressBar">  
        <attr name="roundColor" format="color"/>
        <attr name="roundProgressColor" format="color"/>
        <attr name="roundWidth" format="dimension"></attr>
        <attr name="textColor2" format="color" />
        <attr name="textSize" format="dimension" /> 
        <attr name="max" format="integer"></attr> 
        <attr name="textIsDisplayable" format="boolean"></attr>
        <attr name="style">
            <enum name="STROKE" value="0"></enum>
            <enum name="FILL" value="1"></enum>
        </attr>
    </declare-styleable>
    <declare-styleable name="my_text_view">
        <attr name="contentText" format="string"></attr>
        <attr name="maxLine" format="integer"></attr>
    </declare-styleable>
    <declare-styleable name="my_time_picker_view">
        <attr name="backgroundWheelColor" format="color"></attr>
        <attr name="contentSize" format="dimension"></attr>
        <attr name="isCenterLabel" format="boolean"></attr>
        <attr name="dividerColor" format="color"></attr>
        <attr name="showDay" format="boolean"></attr>
    </declare-styleable>
    <declare-styleable name="wheelview">
        <attr name="gravity">
            <enum name="center" value="17" />
            <enum name="left" value="3" />
            <enum name="right" value="5" />
        </attr>
        <attr name="textsize" format="dimension" />
        <attr name="textColorOut" format="color" />
        <attr name="textColorCenter" format="color" />
        <attr name="dividercolor" format="color" />
    </declare-styleable>
    <!-- 对话框布局 -->
    <declare-styleable name="DialogLayout">
        <attr name="minDialogWidth" format="dimension" />
    </declare-styleable>

</resources>